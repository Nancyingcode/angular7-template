{
  "name": "angular7-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ssr": "npm run build:ssr && npm run serve:ssr",
    "starts": "ng serve --proxy-config proxy.json --aot --port 4206",
    "aot": "ng serve  --aot",
    "builds": "ng build --prod --aot --source-map=false --build-optimizer=false --output-path=prod --base-href /",
    "server": "ng run angular7-template:uni",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng run angular7-template:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/elements": "^7.0.1",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/platform-server": "^7.0.1",
    "@angular/router": "~7.0.0",
    "@ckeditor/ckeditor5-angular": "^1.0.1",
    "@ckeditor/ckeditor5-build-decoupled-document": "^11.2.0",
    "@ckeditor/ckeditor5-dev-webpack-plugin": "^7.0.1",
    "@nguniversal/express-engine": "^7.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "domino": "^2.1.2",
    "js-md5": "^0.7.3",
    "moment": "^2.23.0",
    "ng-zorro-antd": "^1.8.0",
    "rxjs": "~6.3.3",
    "ts-loader": "^5.2.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.3",
